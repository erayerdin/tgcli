{
    "python.linting.enabled": true,
    "python.linting.pycodestyleEnabled": false,
    "python.linting.pylintEnabled": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true,
        ".venv": true,
        ".vscode": true,
        "*.egg-info": true,
        ".pytest_cache": true
    },
    "explorerExclude.backup": null,
    "python.pythonPath": ".venv/bin/python",
    "python.linting.flake8Enabled": true
}
